<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sudoku.Menu">
    <children>
        <ComboBox fx:id="chooseLevel" layoutX="225.0" layoutY="58.0" prefWidth="150.0" promptText="Difficulty">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Easy"/>
                    <String fx:value="Medium"/>
                    <String fx:value="Hard"/>
                </FXCollections>
            </items>
        </ComboBox>
        <Button fx:id="acceptLevel" layoutX="284.0" layoutY="110.0" mnemonicParsing="false" text="OK"
                onAction="#onActionAcceptLevel"/>
        <Button fx:id="startButton" layoutX="255.0" layoutY="266.0" mnemonicParsing="false" prefHeight="47.0"
                prefWidth="90.0" text="Start"
                onAction="#onActionStartButton"/>
    </children>
</Pane>
